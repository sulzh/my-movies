{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": ["last 2 versions", "not dead", "not < 2%"],
				"useBuiltIns": "entry",
				"corejs": 3
			}
		],
		"@babel/preset-react",
		"@babel/preset-typescript"
	],
	"plugins": [
		"react-hot-loader/babel",
		"@babel/plugin-proposal-class-properties"
	]
}
