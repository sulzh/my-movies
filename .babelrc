{
  "presets": [
    [
      "@babel/preset-env", {
        "targets": [
          "last 2 versions",
          "not dead",
          "not < 2%"
        ],
        "useBuiltIns": "entry"
      }
    ],
    "@babel/preset-react",
		"@babel/preset-typescript"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/plugin-proposal-class-properties"
  ]
}